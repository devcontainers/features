                "version": "3"
            }
        }
    },
    "install_dotnet_6_bullseye": {
        "image": "debian:bullseye",
        "features": {
            "dotnet": {
                "version": "6"
            }
        }
    },
    "install_dotnet_6_focal": {
        "image": "ubuntu:focal",
        "features": {
            "dotnet": {
                "version": "6"
            }
        }
    },
    "install_dotnet_7_bullseye": {
        "image": "debian:bullseye",
        "features": {
            "dotnet": {
                "version": "7"
            }
        }
    },
    "install_dotnet_7_jammy": {
        "image": "ubuntu:jammy",
        "features": {
            "dotnet": {
                "version": "7"
            }
        }
