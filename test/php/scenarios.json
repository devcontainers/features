{
    "install_additional_php": {
        "image": "ubuntu:focal",
        "features": {
            "php": {
                "version": "8.1.4",
                "additionalVersions": "8.0.17,8.0.3"
            }
        }
    },
    "install_php_8": {
        "image": "ubuntu:focal",
        "features": {
            "php": {
                "version": "8"
            }
        }
    },
    "install_php_8_2": {
        "image": "ubuntu:focal",
        "features": {
            "php": {
                "version": "8.2"
            }
        }
    },
    "install_only_composer": {
        "image": "mcr.microsoft.com/devcontainers/php:latest",
        "features": {
            "php": {
                "version": "none",
                "installComposer": true
            }
        }
    },
    "test_php_fallback": {
        "image": "ubuntu:focal",
        "features": {
            "php": {
                "version": "latest"
            }
        }
    }
}
