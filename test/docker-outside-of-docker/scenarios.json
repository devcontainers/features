        "image": "mcr.microsoft.com/devcontainers/base",
        "remoteUser": "vscode",
        "features": {
            "docker-outside-of-docker": {
                "version": "latest"
            }
        }
    },
    "docker_build": {
        "image": "mcr.microsoft.com/devcontainers/javascript-node:0-18",
