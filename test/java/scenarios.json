{
    "install_from_non_default_distro": {
        "image": "ubuntu:focal",
        "features": {
            "java": {
                "version": "19",
                "jdkDistro": "open"
            }
        }
    },
    "install_additional_java": {
        "image": "ubuntu:focal",
        "features": {
            "java": {
                "version": "11",
                "additionalVersions": "17,8"
            }
        }
    },
    "install_ant_and_gradle_and_maven_for_user": {
        "image": "ubuntu:focal",
        "remoteUser": "vscode",
        "features": {
            "common-utils": {
                "username": "vscode"
            },
            "java": {
                "version": "latest",
                "installAnt": true,
                "installGradle": true,
                "installMaven": true
            }
        }
    },
    "install_ant_and_gradle_and_maven": {
        "image": "ubuntu:focal",
        "features": {
            "java": {
                "version": "latest",
                "installAnt": true,
                "installGradle": true,
                "installMaven": true
            }
        }
    },
    "install_ant_and_gradle_and_maven_with_specific_version": {
        "image": "ubuntu:focal",
        "features": {
            "java": {
                "version": "latest",
                "installAnt": "true",
                "antVersion": "1.10.12",
                "installGradle": "true",
                "gradleVersion": "6.8.3",
                "installMaven": "true",
                "mavenVersion": "3.6.3"
            }
        }
    },
    "install_non_conventional_version": {
        "image": "ubuntu:focal",
        "features": {
            "java": {
                "version": "21",
                "jdkDistro": "graalce"
            }
        }
    },
    "centos-7": {
        "image": "centos:centos7",
        "features": {
            "java": {}
        }
    },
    "alma-8": {
        "image": "almalinux:8",
        "features": {
            "java": {}
        }
    },
    "alma-8-minimal": {
        "image": "almalinux:8-minimal",
        "features": {
            "java": {}
        }
    },
    "alma-9": {
        "image": "almalinux:9",
        "features": {
            "java": {}
        }
    },
    "alma-9-minimal": {
        "image": "almalinux:9-minimal",
        "features": {
            "java": {}
        }
    }
}