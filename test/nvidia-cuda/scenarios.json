{
    "install_all_options": {
        "image": "debian",
        "features": {
            "nvidia-cuda": {
                "installCudnn": true,
                "installCudnnDev": true,
                "installNvtx": true,
                "installToolkit": true
            }
        }
    },
    "install_cudnn_nvxt_version": {
        "image": "debian",
        "features": {
            "nvidia-cuda": {
                "installCudnn": true,
                "installNvtx": true,
                "cudaVersion": "11.7",
                "cudnnVersion": "8.5.0.96"
            }
        }
    }
}
