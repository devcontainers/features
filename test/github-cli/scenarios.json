{
  "install_git_cli_from_release": {
    "image": "ubuntu:noble",
    "features": {
      "github-cli": {
        "version": "latest",
        "installDirectlyFromGitHubRelease": "false"
      }
    }
  },
  "install_extensions": {
    "image": "ubuntu:noble",
    "features": {
      "github-cli": {
        "version": "latest",
        "extensions": "dlvhdr/gh-dash,github/gh-copilot"
      }
    }
  }
}
