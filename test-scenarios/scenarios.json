{
    "install_jupyterlab": {
        "image": "mcr.microsoft.com/vscode/devcontainers/base:focal",
        "remoteUser": "vscode",
        "features": {
            "common": {
                "username": "vscode"
            },
            "python": {
                "install_jupyterlab": true,
                "configure_jupyterlab_allow_origin": "*"
            }
        }
    },
    "install_additional_ruby": {
        "image": "ubuntu:focal",
        "features": {
            "ruby": {
                "version": "3.1.2",
                "additional_versions": "2.5,3.0.4"
            }
        }
    }
}