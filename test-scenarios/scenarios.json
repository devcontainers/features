{
    "install_python_twice": {
        "image": "mcr.microsoft.com/vscode/devcontainers/base:focal",
        "features": [
            {
                "id": "python",
                "options": {
                    "version": "3.8"
                }
            },
            {
                "id": "python",
                "options": {
                    "version": "3.9",
                    "overrideDefaultVersion": true
                }
            }
        ]
    },
    "install_jupyterlab": {
        "image": "mcr.microsoft.com/vscode/devcontainers/base:focal",
        "features": [
            {
                "id": "common",
                "options": {
                    "username": "vscode"
                }
            },
            {
                "id": "python",
                "options": {
                    "install_jupyterlab": true,
                    "allow_all_origins": true
                }
            }
        ]
    }
}