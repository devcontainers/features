{
    "id": "anaconda",
    "version": "1.0.1",
    "name": "Anaconda",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "4.11.0",
                "4.12.0"
            ],
            "default": "latest",
            "description": "Select or enter an anaconda version."
        }
    },
    "containerEnv": {
        "CONDA_DIR": "/opt/conda",
        "PATH": "${PATH}:${CONDA_DIR}/bin:"
    }
}
