{
    "id": "anaconda",
    "name": "Anaconda",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter an anaconda version."
        }
    },
    "containerEnv": {
        "CONDA_DIR": "/usr/local/conda",
        "PATH": "${PATH}:${CONDA_DIR}/bin:"
    },
    "install": {
        "app": "",
        "file": "install.sh"
    }
}