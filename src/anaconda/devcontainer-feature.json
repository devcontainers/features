{
    "id": "anaconda",
    "version": "1.0.2",
    "name": "Anaconda",
    "documentationURL": "https://github.com/devcontainers/features/tree/main/src/anaconda",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter an anaconda version."
        }
    },
    "containerEnv": {
        "CONDA_DIR": "/usr/local/conda",
        "PATH": "${PATH}:${CONDA_DIR}/bin:"
    }
}
