{
    "id": "anaconda",
    "name": "Anaconda",
    "options": {
        "version": {
            "type": "string",
            "proposals": ["latest"],
            "default": "latest",
            "description": "Select or enter an anaconda version."
        }
    },
    "containerEnv": {
        "ANACONDA_ROOT": "/usr/local/conda",
        "PATH": "${PATH}:${ANACONDA_ROOT}/bin"
    },
    "extensions": [],
    "install": {
        "app": "",
        "file": "install.sh"
    }
}