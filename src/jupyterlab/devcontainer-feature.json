{
    "id": "jupyterlab",
    "name": "Jupyter Lab",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "3.6.2"
            ],
            "default": "latest",
            "description": "Select or enter a jupyterlab version."
        },
        "python_binary": {
            "type": "string",
            "proposals": [
                "python",
                "/usr/local/python/bin/python"
            ],
            "default": "python",
            "description": "Select or enter the python binary path."
        }
    },
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter"
    ],
    "install": {
        "app": "",
        "file": "install.sh"
    }
}